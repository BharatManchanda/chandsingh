{
    "functions": {
      "api/test.js": {
        "maxDuration": 60 
      },
      "api/*.js": {
        "maxDuration": 60 
      },
      "src/api/*.js": {
        "maxDuration": 60
      }
    },
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}